@prefix ja:      <http://jena.hpl.hp.com/2005/11/Assembler#> .
@prefix :        <#> .

########################

[] rdf:type fuseki:Server ;
   fuseki:services (
     <#cornell>
     <#harvard>
     <#stanford>
     <#cornell_share_vde>
     <#stanford_share_vde>
     <#ucsd_share_vde>
     <#combined>
     <#stanford2>
     <#dbpedia>
     <#viaf>
     <#fast>
     <#geonames>
     <#grid>
     <#loc_demographics>
     <#loc_genre>
     <#loc_names>
     <#loc_performance>
     <#loc_subjects>
     <#loc_works_instances>
     <#getty_all>
     <#getty_aat>
     <#getty_tgn>
     <#getty_ulan>
     <#nalt>
     <#agrovoc>
     <#wikidata>
     <#mesh>
     <#share_vde_frick>
     <#share_vde_alberta>
     <#share_vde_duke>
     <#share_vde_colorado>
     <#share_vde_cornell>
   ) .

########################

# Declaration additional assembler items.
[] ja:loadClass "org.apache.jena.tdb.TDB" .

# TDB
tdb:DatasetTDB  rdfs:subClassOf  ja:RDFDataset .
tdb:GraphTDB    rdfs:subClassOf  ja:Model .

########################

<#cornell>  rdf:type fuseki:Service ;
    fuseki:name              "cornell" ;       # http://host:port/tdb
