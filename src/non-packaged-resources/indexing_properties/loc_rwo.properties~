	        FILTER (isBlank(?o)) \n\
	      } \n\
		  OPTIONAL { \n\
            ?s mads:hasSource ?src . \n\
            ?src ?sx ?source . \n\
          } \n\
		  OPTIONAL { \n\
            ?rws mads:hasAffiliation ?aff . \n\
            ?aff mads:organization ?org . \n\
            ?org mads:authoritativeLabel ?lab . \n\
          } \n\
	      OPTIONAL { \n\
	      	?rws ?rwp ?rwo. \n\
	        OPTIONAL { \n\
	           ?rwo ?rp ?rs \n\
	        } \n\
          } \n\
	    } \n\
    UNION \n\
    	{ \n\
		  ?s ?p ?o . \n\
          ?s <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://www.loc.gov/mads/rdf/v1#CorporateName> . \n\
	      ?s mads:identifiesRWO ?rws. \n\
		  OPTIONAL { \n\
            ?rws ?rwp ?rwo. \n\
          } \n\
		  OPTIONAL { \n\
            ?s mads:hasSource ?src . \n\
            ?src ?sx ?source . \n\
          } \n\
 		  OPTIONAL { \n\
            ?rws mads:hasAffiliation ?aff . \n\
            ?aff mads:organization ?org . \n\
            ?org mads:authoritativeLabel ?lab . \n\
          } \n\
          OPTIONAL { \n\
            ?o ?q ?r2 \n\
            FILTER (isBlank(?o2)) \n\
          } \n\
    	} \n\
    UNION \n\
        { \n\
          ?s ?p ?o . \n\
          ?s <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://www.loc.gov/mads/rdf/v1#Title> . \n\
          ?s mads:identifiesRWO ?rws. \n\
          OPTIONAL { \n\
            ?rws ?rwp ?rwo. \n\
          } \n\
		  OPTIONAL { \n\
            ?s mads:hasSource ?src . \n\
            ?src ?sx ?source . \n\
          } \n\
		  OPTIONAL { \n\
            ?rws mads:hasAffiliation ?aff . \n\
            ?aff mads:organization ?org . \n\
            ?org mads:authoritativeLabel ?lab . \n\
          } \n\
          OPTIONAL { \n\
            ?o ?q ?r2 \n\
            FILTER (isBlank(?o2)) \n\
          } \n\
        } \n\
    UNION \n\
        { \n\
          ?s ?p ?o . \n\
          FILTER NOT EXISTS { ?s <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://www.loc.gov/mads/rdf/v1#PersonalName> } \n\
          FILTER NOT EXISTS { ?s <http://www.w3.org/1999/02/22-rdf-syntax-ns#type> <http://www.loc.gov/mads/rdf/v1#CorporateName> } \n\
